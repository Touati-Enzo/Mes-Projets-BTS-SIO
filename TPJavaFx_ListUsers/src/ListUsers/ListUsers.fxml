<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="566.0" prefWidth="757.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ListUsers.ListUsersController">
	<children>
		<Text layoutX="212.0" layoutY="85.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Liste des utilisateurs">
			<font>
				<Font name="System Bold" size="28.0" />
			</font>
		</Text>
		<TableView fx:id="tableUser" layoutX="102.0" layoutY="130.0" prefHeight="345.0" prefWidth="555.0">
			<columns>
				<TableColumn fx:id="columnName" prefWidth="166.0" text="Nom" />
				<TableColumn fx:id="columnEmail" prefWidth="212.0" text="Adresse Email" />
				<TableColumn fx:id="columnDepartment" prefWidth="176.0" text="Département" />
			</columns>
		</TableView>
		<Button fx:id="btnLoad" layoutX="268.0" layoutY="501.0" mnemonicParsing="false" onAction="#loadDataFromDatabase" prefHeight="30.0" prefWidth="221.0" style="-fx-background-color: Royalblue;" text="Afficher Informations" textFill="WHITE">
			<font>
				<Font name="System Bold" size="16.0" />
			</font>
		</Button>
		<Button fx:id="btnDeconnection" layoutX="596.0" layoutY="33.0" mnemonicParsing="false" onAction="#Deconnection" prefHeight="24.0" prefWidth="120.0" style="-fx-background-color: DarkRed;" text="Se déconnecter" textFill="WHITE" />
	</children>
</AnchorPane>
